<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.callcenterpropre"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.callcenterpropre.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.callcenterpropre.creneauHoraire"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.callcenterpropre.Planing"
            android:label="@string/title_activity_planing" >
        </activity>
        <activity
            android:name="com.example.callcenterpropre.Motdepasse"
            android:label="@string/title_activity_motdepasse" >
        </activity>
         <activity
            android:name="com.example.callcenterpropre.MyCalendarActivity"
            android:label="@string/title_activity_my_calendar_activity" >
        </activity>
        <activity
            android:name="com.example.callcenterpropre.TestRedirection"
            android:label="@string/title_activity_my_calendar_activity" >
        </activity>
    </application>

</manifest>
